  
<p id="notice"><%= notice %></p>

<h1>Clients</h1>

<table>
  <thead>
    <tr>
      <th>Cpf cnpj</th>
      <th>Name</th>
      <th>Email</th>
      <th>Address</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @clients.each do |client| %>
      <tr>
        <td><%= client.cpf_cnpj %></td>
        <td><%= client.name %></td>
        <td><%= client.email %></td>
        <td><%= client.address_id %></td>
        <td><%= link_to 'Show', client, id: "s-#{client.cpf_cnpj}" %></td>
        <td><%= link_to 'Edit', edit_client_path(client), id: "e-#{client.cpf_cnpj}"%></td>
        <td><%= link_to 'Destroy', client, id: "d-#{client.cpf_cnpj}", method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Client', new_client_path %>
